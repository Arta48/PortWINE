#!/bin/bash
# Author: PortWINE-Linux.ru
. "$(dirname $(readlink -f "$0"))/runlib"
START_PORTWINE
export WINEDEBUG="fixme-all" 
if [ ! -z ${optirun_on} ]
then
    ${optirun_on} "${PROTONRUN}" "run" "explorer" & KILL9_WINEDEVICE >&2
else
    "${PROTONRUN}" "run" "explorer" & KILL9_WINEDEVICE >&2
fi
STOP_PORTWINE
