#!/bin/bash
# Author: PortWINE-Linux.ru
. "$(dirname $(readlink -f "$0"))/runlib"
"${WINESERVER}" -k
START_PORTWINE
if [ ! -z ${optirun_on} ]; then
    ${optirun_on} "${PROTONRUN}" "run" "${gamestart}" ${launch_parameters}
else
   "${PROTONRUN}" "run" "${gamestart}" ${launch_parameters} 
fi
STOP_PORTWINE
